<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AzamRealEstate.Views.HomeView"
             Title="HomeView">


    <!--<FlyoutPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:local="clr-namespace:YourNamespace"
            x:Class="YourNamespace.MainPage">-->

        <FlyoutPage.Flyout>
            <!-- Define the navigation drawer content here -->
            <ContentPage Title="Menu">
                <StackLayout>
                    <Label Text="Home" />
                    <Label Text="Profile" />
                    <Label Text="Settings" />
                </StackLayout>
            </ContentPage>
        </FlyoutPage.Flyout>

        <FlyoutPage.Detail>
            <NavigationPage>
                <!-- Define the main content here -->
                <x:Arguments>
                    <ContentPage>
                        <Label Text="Welcome to the App!" HorizontalOptions="Center" VerticalOptions="CenterAndExpand" />
                </x:Arguments>
                </x:Arguments>
            </NavigationPage>
        </FlyoutPage.Detail>

    </FlyoutPage>
</FlyoutPage>



